<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE validators PUBLIC 
"-//OpenSymphony Group//XWork Validator 1.0.2//EN" 
"http://www.opensymphony.com/xwork/xwork-validator-1.0.2.dtd">

<validators>

	<field name="form.password">
		<field-validator type="requiredstring">
			<message key="errors.web.user.fields.password.obligatorio" />
		</field-validator>
	</field>

	<field name="form.checkPassword">
		<field-validator type="requiredstring">
			<message key="errors.web.user.fields.passwordcheck.obligatorio" />
		</field-validator>
	</field>

  <validator type="expression">
       <param name="expression">form.password == form.checkPassword</param>
       <message key="usuario.errors.checkPassword" />
  </validator>

</validators>