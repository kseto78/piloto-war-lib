<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="es.mpr.plataformamensajeria.beans">
	<class name="es.mpr.plataformamensajeria.beans.PlanificacionBean" table="TBL_SERVIDORES">
  		<id name="planificacionId" column="PLANIFICACIONID">
    		<generator class="native"/>
    	</id>
        <property name="servidorId" column="SERVIDORID" />
        <property name="servicioId" column="SERVICIOID" />
    	<property name="tipoPlanificacionId" column="TIPOPLANIFICACIONID" />
    	<property name="lunes" column="L" />
    	<property name="martes" column="M" />
		<property name="miercoles" column="X" />
		<property name="jueves" column="J" />
		<property name="viernes" column="V" />
		<property name="sabado" column="S" />
		<property name="domingo" column="D" />
		<property name="horaDesde" column="HORADESDE" />
		<property name="horaHasta" column="HORAHASTA" />
		<property name="activo" column="ACTIVO" />
		<property name="fechaCreacion" column="FECHACREACION" />
		<property name="creadoPor" column="CREADOPOR" />
		<property name="modificadoPor" column="MODIFICADOPOR" />
		<property name="fechaModificacion" column="FECHAMODIFICACION" />
		<property name="externalId" column="EXTERNALID" />
	</class>
	<sql-query name="deletePlanificacionPKG" callable="false">
		
	     select PKG_PLANIFICACION.deletePlanificacion(?) from dual 
      
	</sql-query>
</hibernate-mapping>