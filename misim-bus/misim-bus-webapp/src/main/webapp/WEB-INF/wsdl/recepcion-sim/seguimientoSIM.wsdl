<?xml version="1.0" encoding="UTF-8"?>
<wsdl:definitions xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/" xmlns:xsd="http://www.w3.org/2001/XMLSchema"  
	xmlns:soap="http://schemas.xmlsoap.org/wsdl/soap/" 
	xmlns:impl="http://misim.redsara.es/misim-bus-webapp/"  
	xmlns:ns1="http://misim.redsara.es/misim-bus-webapp/peticion" 
	xmlns:ns2="http://misim.redsara.es/misim-bus-webapp/respuesta"
	targetNamespace="http://misim.redsara.es/misim-bus-webapp/">
	<wsdl:types>
		<schema xmlns="http://www.w3.org/2001/XMLSchema" targetNamespace="http://www.map.es/xml-schemas">
			<import namespace="http://misim.redsara.es/misim-bus-webapp/peticion" schemaLocation="peticionSeguimiento.xsd"/>
			<import namespace="http://misim.redsara.es/misim-bus-webapp/respuesta" schemaLocation="respuestaSeguimiento.xsd"/>
		</schema>
	</wsdl:types>
	<wsdl:message name="consultarEstadoPeticion">
		<wsdl:part name="peticion" element="ns1:PeticionEstado"/>
	</wsdl:message>
	<wsdl:message name="consultarEstadoRespuesta">
		<wsdl:part name="respuesta" element="ns2:Respuesta"/>
	</wsdl:message>
	<wsdl:message name="consultarHistorialPeticion">
		<wsdl:part name="peticion" element="ns1:PeticionHistorico"/>
	</wsdl:message>
	<wsdl:message name="consultarHistorialRespuesta">
		<wsdl:part name="respuesta" element="ns2:Respuesta"/>
	</wsdl:message>
	<wsdl:portType name="SeguimientoMensajesServicePortType">
		<wsdl:operation name="consultarEstado">
			<wsdl:input name="PeticionEstado" message="impl:consultarEstadoPeticion"/>
			<wsdl:output name="RespuestaEstado" message="impl:consultarEstadoRespuesta"/>
		</wsdl:operation>
		<wsdl:operation name="consultarHistorial">
			<wsdl:input name="PeticionHistorial" message="impl:consultarHistorialPeticion"/>
			<wsdl:output name="RespuestaHistorial" message="impl:consultarHistorialRespuesta"/>
		</wsdl:operation>
	</wsdl:portType>
	<wsdl:binding name="SeguimientoMensajesServiceBinding" type="impl:SeguimientoMensajesServicePortType">
		<soap:binding style="document" transport="http://schemas.xmlsoap.org/soap/http"/>
		<wsdl:operation name="consultarEstado">
			<soap:operation soapAction="consultarEstado" style="document"/>
			<wsdl:input>
				<soap:body parts="peticion" use="literal"/>
			</wsdl:input>
			<wsdl:output>
				<soap:body parts="respuesta" use="literal"/>
			</wsdl:output>
		</wsdl:operation>
		<wsdl:operation name="consultarHistorial">
			<soap:operation soapAction="consultarHistorial" style="document"/>
			<wsdl:input>
				<soap:body parts="peticion" use="literal"/>
			</wsdl:input>
			<wsdl:output>
				<soap:body parts="respuesta" use="literal"/>
			</wsdl:output>
		</wsdl:operation>
	</wsdl:binding>
	<wsdl:service name="SeguimientoMensajesService">
		<wsdl:port name="SeguimientoMensajesServicePort" binding="impl:SeguimientoMensajesServiceBinding">
<!-- 			<soap:address location="http://des-misim.redsara.es:8081/SeguimientoMensajesService"/> -->
			<soap:address location="/misim-bus-webapp/SeguimientoMensajesService"/>
		</wsdl:port>
	</wsdl:service>
</wsdl:definitions>
