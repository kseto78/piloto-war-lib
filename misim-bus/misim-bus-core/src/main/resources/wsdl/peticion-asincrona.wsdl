<?xml version="1.0" encoding="UTF-8"?>
<wsdl:definitions xmlns="http://schemas.xmlsoap.org/wsdl/"
	xmlns:apachesoap="http://xml.apache.org/xml-soap" xmlns:impl="http://www.map.es/xml-schemas"
	xmlns:ns="http://intermediacion.redsara.es/scsp/esquemas/datosespecificos"
	xmlns:ns1="http://intermediacion.redsara.es/scsp/esquemas/V3/peticion"
	xmlns:ns2="http://intermediacion.redsara.es/scsp/esquemas/V3/respuesta"
	xmlns:ns3="http://intermediacion.redsara.es/scsp/esquemas/V3/confirmacionPeticion"
	xmlns:ns4="http://intermediacion.redsara.es/scsp/esquemas/V3/solicitudRespuesta"
	xmlns:soapenc="http://schemas.xmlsoap.org/soap/encoding/" xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/"
	xmlns:wsdlsoap="http://schemas.xmlsoap.org/wsdl/soap/" xmlns:xsd="http://www.w3.org/2001/XMLSchema"
	targetNamespace="http://www.map.es/xml-schemas">
	<wsdl:types>
		<schema targetNamespace="http://www.map.es/xml-schemas" xmlns="http://www.w3.org/2001/XMLSchema">
			<import namespace="http://intermediacion.redsara.es/scsp/esquemas/V3/peticion"
				schemaLocation="peticion.xsd" />
			<import namespace="http://intermediacion.redsara.es/scsp/esquemas/V3/respuesta"
				schemaLocation="respuesta.xsd" />
			<import
				namespace="http://intermediacion.redsara.es/scsp/esquemas/V3/confirmacionPeticion"
				schemaLocation="confirmacion-peticion.xsd" />
			<import
				namespace="http://intermediacion.redsara.es/scsp/esquemas/V3/solicitudRespuesta"
				schemaLocation="solicitud-respuesta.xsd" />
		</schema>
	</wsdl:types>
	<wsdl:message name="PeticionReq">
		<wsdl:part name="peticion" element="ns1:Peticion" />
	</wsdl:message>
	<wsdl:message name="PeticionRes">
		<wsdl:part name="respuesta" element="ns2:Respuesta" />
	</wsdl:message>
	<wsdl:message name="RespuestaAsincrona">
		<wsdl:part name="confirmacionPeticion" element="ns3:ConfirmacionPeticion" />
	</wsdl:message>
	<wsdl:message name="SolicitudRespuesta">
		<wsdl:part name="solicitudRespuesta" element="ns4:SolicitudRespuesta" />
	</wsdl:message>
	<wsdl:portType name="MICCAsincrono">
		<wsdl:operation name="peticionAsincrona">
			<wsdl:input name="Peticion" message="impl:PeticionReq" />
			<wsdl:output name="CPeticion" message="impl:RespuestaAsincrona" />
		</wsdl:operation>
		<wsdl:operation name="solicitudRespuesta">
			<wsdl:input name="SRespuesta" message="impl:SolicitudRespuesta" />
			<wsdl:output name="Respuesta" message="impl:PeticionRes" />
		</wsdl:operation>
	</wsdl:portType>
	<wsdl:binding name="MICCAsincronoSOAPBinding" type="impl:MICCAsincrono">
		<wsdlsoap:binding style="document"
			transport="http://schemas.xmlsoap.org/soap/http" />
		<wsdl:operation name="peticionAsincrona">
			<wsdlsoap:operation soapAction="peticionAsincrona"
				style="document" />
			<wsdl:input>
				<wsdlsoap:body parts="peticion" use="literal" />
			</wsdl:input>
			<wsdl:output>
				<wsdlsoap:body parts="confirmacionPeticion" use="literal" />
			</wsdl:output>
		</wsdl:operation>
		<wsdl:operation name="solicitudRespuesta">
			<wsdlsoap:operation soapAction="solicitudRespuesta"
				style="document" />
			<wsdl:input>
				<wsdlsoap:body parts="solicitudRespuesta" use="literal" />
			</wsdl:input>
			<wsdl:output>
				<wsdlsoap:body parts="respuesta" use="literal" />
			</wsdl:output>
		</wsdl:operation>
	</wsdl:binding>
	<wsdl:service name="MICCAsincrono">
		<wsdl:port name="MICCAsincrono" binding="impl:MICCAsincronoSOAPBinding">
			<wsdlsoap:address location="http://des-carpetaciudadana.redsara.es/micc-bus-webapp/MICCAsincrono" />
		</wsdl:port>
	</wsdl:service>
</wsdl:definitions>
